    <notificationGroup id="Wrote Baseline" displayType="BALLOON"/>

    <lang.inspectionSuppressor
        language="Groovy"
        implementationClass="com.android.tools.idea.lint.common.AndroidLintJcenterInspectionSuppressor"/>
  </extensions>
</idea-plugin>
